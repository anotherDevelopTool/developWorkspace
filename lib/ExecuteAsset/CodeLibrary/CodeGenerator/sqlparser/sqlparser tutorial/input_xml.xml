select RSS_INFOM.RSM_FEEDDATA as RSM_FEEDDATA, RSS_INFOM.RSM_UPDATE_TIME as RSM_UPDATE_TIME, 
RSS_DATA.RSS_SID as RSS_SID, RSS_DATA.USR_SID as USR_SID, RSS_DATA.RSD_TITLE as RSD_TITLE, 
RSS_DATA.RSD_URL_FEED as RSD_URL_FEED, RSS_DATA.RSD_URL as RSD_URL, RSS_DATA.RSD_VIEW as RSD_VIEW, 
RSS_DATA.RSD_FEED_COUNT as RSD_FEED_COUNT, RSS_DATA.RSD_AUTH as RSD_AUTH, RSS_DATA.RSD_AUTH_ID 
as RSD_AUTH_ID, RSS_DATA.RSD_AUTH_PSWD as RSD_AUTH_PSWD, COALESCE(RSS_POSITION_MAIN.RPM_POSITION, 
1) as RPM_POSITION from RSS_INFOM, RSS_DATA left join RSS_POSITION_MAIN on RSS_DATA.RSS_SID 
= RSS_POSITION_MAIN.RSS_SID and RSS_DATA.USR_SID = RSS_POSITION_MAIN.USR_SID where RSS_INFOM.RSS_SID=RSS_DATA.RSS_SID 
and RSS_DATA.USR_SID=101 and RSS_DATA.RSD_MAIN_VIEW=0 order by COALESCE(RSS_POSITION_MAIN.RPM_ORDER, 
0) 